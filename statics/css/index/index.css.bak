.index_box{
			height:600px;
			width:100%;
			margin:0px;
			padding:0px;
			background:url('../../images/index/back.png');
			font-family:'微软雅黑';
		}
		.content{
			width:560px;
			height:200px;
			margin:136px auto 270px auto;
		
		}
		.content img{
			position:relative;
			top:-50px;
			margin-left:32%;
		}
		.jichang_title{
			color:#1969c0;
			border-top:1px solid #1969c0;
			border-left:1px solid #1969c0;
			border-right:1px solid #1969c0;
			border-bottom:1px solid #fff;
			position:relative;
			top:1px;
			float:left;
			background-color:#fff;
			width:80px;
			height:37px;
			text-align:center;
			line-height:37px;
			font-size:18px;
			-moz-border-radius-topleft: 4px;
			-moz-border-radius-topright: 4px;
			-webkit-border-top-left-radius: 4px;
			-webkit-border-top-right-radius: 4px;
			border-top-left-radius: 4px;
			border-top-right-radius: 4px;

		}
		.qiye_title{
			float:left;
			background-color:#fff;
			width:80px;
			height:37px;
			text-align:center;
			line-height:37px;
			color:#717171;
			font-size:18px;
				-moz-border-radius-topleft: 4px;
			-moz-border-radius-topright: 4px;
			-webkit-border-top-left-radius: 4px;
			-webkit-border-top-right-radius: 4px;
			border-top-left-radius: 4px;
			border-top-right-radius: 4px;
		}
		.qiye,.jichang{
			height:37px;
			width:456px;
			font-size:12px;
			color:#a4a3a3;
			float:left;
			border:1px solid #1969c0;
		}
		.tijiao{
			height:41px;
			width:96px;
			color:#fff;
			font-size:20px;
			border:0px;
			background-color:#1969c0;
		}